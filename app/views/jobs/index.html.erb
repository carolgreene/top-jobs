<h1>Jobs Index</h1>

<% @jobs.each do |job| %> 
    <%= link_to "#{job.title}", job_path(job) %> | 
    <%= job.company_name %> | <%= job.location %> |
    Date Posted: <%= job.created_at.strftime("%m/%d/%y") %> <br>
    <% end %><br>
    <br>
    <br>

<%= link_to "Sign In", signin_path %> |
<%= link_to "Sign Up", new_user_path %> |
<%= link_to "Log Out", signout_path %>