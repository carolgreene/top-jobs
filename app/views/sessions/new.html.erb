<h2>Sign In</h2>




<%= form_for :user, url: signin_path do |f| %>
    

    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :password, "Password:" %><br>
    <%= f.password_field :password %><br>
    <br>
    <%= f.submit "Sign In" %>
    <% end %>
    <br>
    <%= link_to "Home", root_path %> | <%= link_to "All Jobs", jobs_path %> </div>
    
